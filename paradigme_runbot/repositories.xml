<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="odoo_repo" model="runbot.repo">
        <field name="name">odoo</field>
        <field name="project_id" ref="runbot.main_project"/>
        <field name="modules">l10n_*</field>
        <field name="server_files">odoo-bin</field>
        <field name="addons_path">odoo/addons,addons</field>
    </record>

    <record id="odoo_remote" model="runbot.remote">
        <field name="name">https://github.com/stanadigme/odoo.git</field>
        <field name="repo_id" ref="paradigme_runbot.odoo_repo"/>
    </record>

</odoo>
